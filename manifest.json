{
    "description": "Debugging utilities on global devcmd.",
    "manifest_version": 2,
    "name": "Dev Commands",
    "version": "0.0.1",
    "author": "Beothorn",
    "homepage_url": "https://github.com/beothorn/devtools_snippets",
    "icons": {
        "48": "icons/logo-48.png"
    },
    "content_scripts": [
        {"matches": ["<all_urls>"], "js": ["inject.js"]}
    ],
    "permissions": [
        "<all_urls>"
    ],
    "web_accessible_resources":["commands.js"]
}
